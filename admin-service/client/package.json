{
  "name": "@rhysley/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel announce watch serve",
    "announce": "echo 'Client URL: http://localhost:7002/'",
    "watch": "vite build --watch",
    "serve": "vite preview --port 7002 --host",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/genai": "^1.9.0",
    
    "marked": "^16.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "dompurify": "^3.0.6"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/marked": "^6.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/dompurify": "^3.0.5",
    "npm-run-all": "^4.1.5"
  }
}



                                                                                                                                                     